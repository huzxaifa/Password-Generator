{
  "name": "Password Manager Extension",
  "author": "Your Name",
  "version": "0.1.0",
  "description": "Your extension description",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_icon": {
      "16": "./icons/logo16.png",
      "48": "./icons/logo48.png",
      "128": "./icons/logo128.png"
    },
    "default_title": "Password Manager",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./icons/logo16.png",
    "48": "./icons/logo48.png",
    "128": "./icons/logo128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 3
}